{
  "name": "@trig-app/marketing",
  "private": true,
  "description": "Trig Marketing",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "gatsby build && netlify-lambda build src/lambda",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambda",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --runInBand",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "coverage": "open-cli coverage/lcov-report/index.html",
    "t:oc": "npm run test:coverage && npm run coverage",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/lambda",
    "serve": "gatsby serve",
    "clean": "gatsby clean && rimraf package-lock.json && rimraf node_modules && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "precise-commits",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@sendgrid/mail": "^6.5.0",
    "@trig-app/constants": "^0.1.4",
    "@trig-app/core-components": "^0.1.48",
    "@trig-app/themes": "^0.1.3",
    "gatsby": "^2.18.17",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-disqus": "^1.1.4",
    "gatsby-plugin-google-tagmanager": "^2.1.24",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-source-prismic": "^2.3.0-alpha.3",
    "normalize.css": "^8.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^6.0.0-beta.2",
    "react-stripe-elements": "^6.0.1",
    "stripe": "^8.17.0",
    "styled-components": "^4.4.1",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.2",
    "@trig-app/eslint-config": "^1.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.23",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gatsby-plugin-offline": "^3.0.21",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.3.3",
    "gatsby-plugin-styled-components": "^3.1.13",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-sharp": "^2.3.5",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.5.0",
    "mockdate": "^2.0.5",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "open-cli": "^5.0.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "svg-sprite-loader": "^4.1.6",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
